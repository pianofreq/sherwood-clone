<script>
    window.customWidgetsFunctions["b2f78025607d46d19174723879f60966~6"] = function (element, data, api) {
        //create FB root element only once. This way re-rendering the element will not lose root.
if(!document.getElementById('fb-root')) {
    console.log("no fb root");
    var fbDiv = document.createElement('div');
    fbDiv.id = "fb-root";
    document.body.appendChild(fbDiv);
}

//turn load SDK into a function to be called on page ready
var loadFB = function(d, s, id, cb) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=1822295708016537";
  fjs.parentNode.insertBefore(js, fjs);
};

//attach event listner to runOnReady API. This loads the script every time there's a page change
dmAPI.runOnReady('fb',function(){
    //check if FB object is loaded and exists (improves performance)
    if (typeof(FB) === "object") {
        //load FB page if FB SDK exists. 
        //Requests only FB page within custom widget to be rendered, incase multiple are on the page.
        FB.XFBML.parse(element)
    } else {
        loadFB(document, 'script', 'facebook-jssdk');
    }
})

    };
</script>
<script>
    window.customWidgetsFunctions["d1ee5d5bf037471ea1770683731e577a~22"] = function (element, data, api) {
        $(document).ready(function(){
    $.getScript('https://widget.hibu.us/static/js/index.js')
    .done(function(){
        dmAPI.runOnReady('hibu_header_reviews',function(){
            window.renderApp();
        });
        
    })
    .fail(function(){
        console.log("failed to loaded header reviews js file")
    });
})


    };
</script>
<script type="text/javascript">

    var d_version = "production_5519";
    var build = "2025-06-05T12_49_29";
    window['v' + 'ersion'] = d_version;

    function buildEditorParent() {
        window.isMultiScreen = true;
        window.editorParent = {};
        window.previewParent = {};
        window.assetsCacheQueryParam = "?version=2025-06-05T12_49_29";
        try {
            var _p = window.parent;
            if (_p && _p.document && _p.$ && _p.$.dmfw) {
                window.editorParent = _p;
            } else if (_p.isSitePreview) {
                window.previewParent = _p;
            }
        } catch (e) {

        }
    }

    buildEditorParent();
</script>
