//turn load SDK into a function to be called on page ready
var loadFB = function(d, s, id, cb) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=1822295708016537";
  fjs.parentNode.insertBefore(js, fjs);
};

//attach event listner to runOnReady API. This loads the script every time there's a page change
dmAPI.runOnReady('fb',function(){
    //check if FB object is loaded and exists (improves performance)
    if (typeof(FB) === "object") {
        //load FB page if FB SDK exists. 
        //Requests only FB page within custom widget to be rendered, incase multiple are on the page.
        FB.XFBML.parse(element)
    } else {
        loadFB(document, 'script', 'facebook-jssdk');
    }
})

    };
</script>
